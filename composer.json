{
    "name": "yaroslawww/nova-external-login",
    "description": "Laravel Nova resource tool that allows a user or other resource to log into an external control panel or application.",
    "keywords": [
        "external",
        "login"
    ],
    "homepage": "https://github.com/yaroslawww/nova-external-login",
    "license": "MIT",
    "authors": [
        {
            "name": "Yaroslav Georgitsa",
            "email": "yaroslav.georgitsa@gmail.com",
            "homepage": "https://github.com/yaroslawww",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "NovaExternalLogin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaExternalLogin\\ToolServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
